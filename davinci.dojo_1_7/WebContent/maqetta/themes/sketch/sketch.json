                "Normal": {
					"selectors": {
						".sketch .dijitAccordionContainer": [
							"background-color",
							"border-width",
							"border-style",
							"border-color",
							"padding-top",
							"padding-right",
							"padding-bottom",
							"padding-left",
							"margin-top",
