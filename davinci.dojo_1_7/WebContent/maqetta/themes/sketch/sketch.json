                            	".sketch .dijitAccordionInnerContainerSelectedActive .dijitAccordionContainer-child":[
                            		"background-color",
									"border-color",
									"border-style",
									"border-width",
									"-webkit-box-shadow:",
									"-moz-box-shadow" 
                            	] 
							},
                            "query": ".dijitAccordionInnerContainerActive",
							"simulate": "dijitSelectedActive dijitAccordionInnerContainerSelectedActive"
