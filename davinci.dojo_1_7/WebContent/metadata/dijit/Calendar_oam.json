{
    "id": "http://dojotoolkit.org/dijit/Calendar",
    "name": "dijit.Calendar",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dijit.Calendar');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../"
        }
    },
    "property": {
        "name": {
            "datatype": "string",
            "title": "Name"
        },
        "value": {
            "datatype": "string",
            "format": "date",
            "title": "Value"
        },
        "dayWidth": {
            "datatype": "string",
            "option": [
                {
                    "value": "narrow"
                },
                {
                    "value": "wide"
                },
                {
                    "value": "abbr"
                }
            ],
            "defaultValue": "narrow",
            "title": "Day Width"
        },
        "datePackage": {
            "datatype": "string",
            "description": "JavaScript namespace to find Calendar routines.  Uses Gregorian Calendar routines\nat dojo.date by default.",
            "hidden": false
        }
    },
    "content": "<input></input>"
}