{
    "id": "http://dojotoolkit.org/dijit/PopupMenuItem",
    "name": "dijit.PopupMenuItem",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dijit.PopupMenuItem');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../"
        }
    },
    "property": {
        "label": {
            "datatype": "string",
            "title": "Label"
        },
        "popup": {
            "datatype": "object",
            "title": "Popup"
        },
        "iconClass": {
            "datatype": "string",
            "title": "Icon Class"
        },
        "accelKey": {
            "datatype": "string",
            "title": "Shortcut Key"
        },
        "disabled": {
            "datatype": "boolean",
            "title": "Disabled"
        }
    },
    "content": "<span></span>"
}