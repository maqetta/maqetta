{
    "id": "http://dojotoolkit.org/dijit/form/Form",
    "name": "dijit.form.Form",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dijit.form.Form');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../"
        }
    },
    "property": {
        "name": {
            "datatype": "string",
            "title": "Name"
        },
        "action": {
            "datatype": "string",
            "title": "Action"
        },
        "method": {
            "datatype": "string",
            "option": [
                {
                    "value": "POST"
                },
                {
                    "value": "GET"
                }
            ],
            "title": "Method"
        },
        "encType": {
            "datatype": "string",
            "option": [
                {
                    "value": "multipart/form-data"
                }
            ],
            "title": "Encoding Type"
        },
        "accept-charset": {
            "datatype": "string",
            "title": "Accept Charset"
        },
        "accept": {
            "datatype": "string",
            "title": "Accept"
        },
        "target": {
            "datatype": "string",
            "option": [
                {
                    "value": "_blank"
                },
                {
                    "value": "self"
                },
                {
                    "value": "_top"
                },
                {
                    "value": "_parent"
                }
            ],
            "title": "Target"
        }
    },
    "content": "<form></form>"
}