{
    "id": "http://dojotoolkit.org/dijit/form/HorizontalRule",
    "name": "dijit.form.HorizontalRule",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dijit.form.Slider');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../"
        }
    },
    "property": {
        "count": {
            "datatype": "number",
            "defaultValue": 3,
            "title": "Count"
        },
        "ruleStyle": {
            "datatype": "string",
            "title": "Rule Style"
        },
        "container": {
            "datatype": "string",
            "description": "For HorizontalSlider, this is either \"topDecoration\" or \"bottomDecoration\",\nand indicates whether this rule goes above or below the slider.",
            "hidden": false
        }
    },
    "content": "<span></span>"
}