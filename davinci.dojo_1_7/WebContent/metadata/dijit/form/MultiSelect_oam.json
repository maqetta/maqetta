{
    "id": "http://dojotoolkit.org/dijit/form/MultiSelect",
    "name": "dijit.form.MultiSelect",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dijit.form.MultiSelect');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../"
        }
    },
    "property": {
        "type": {
            "datatype": "string",
            "defaultValue": "text",
            "hidden": true
        },
        "name": {
            "datatype": "string",
            "title": "Name"
        },
        "alt": {
            "datatype": "string",
            "hidden": true
        },
        "tabIndex": {
            "datatype": "string",
            "defaultValue": "0",
            "title": "Tab Index"
        },
        "disabled": {
            "datatype": "boolean",
            "title": "Disabled"
        },
        "readOnly": {
            "datatype": "boolean",
            "title": "Read Only"
        },
        "intermediateChanges": {
            "datatype": "boolean",
            "hidden": true
        },
        "size": {
            "datatype": "number",
            "defaultValue": 7,
            "title": "Size"
        },
        "scrollOnFocus": {
            "datatype": "boolean",
            "description": "On focus, should this widget scroll into view?",
            "hidden": false,
            "defaultValue": true
        }
    },
    "content": "<select multiple='true'></select>"
}