{
    "id": "http://dojotoolkit.org/dijit/layout/StackController",
    "name": "dijit.layout.StackController",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dijit.layout.StackContainer');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../"
        }
    },
    "property": {
        "containerId": {
            "datatype": "string",
            "title": "Container ID"
        },
        "buttonWidget": {
            "datatype": "string",
            "description": "The name of the button widget to create to correspond to each page"
        },
        "isContainer": {
            "datatype": "boolean",
            "description": "Just a flag indicating that this widget descends from dijit._Container",
            "hidden": true,
            "defaultValue": true
        }
    },
    "content": "<span></span>"
}