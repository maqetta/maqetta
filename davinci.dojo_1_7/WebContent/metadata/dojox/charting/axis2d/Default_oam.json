{
    "id": "http://dojotoolkit.org/dojox/charting/axis2d/Default",
    "name": "dojox.charting.axis2d/Default",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.charting.axis2d.Default');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../../"
        }
    },
    "property": {
        "vertical": {
            "datatype": "boolean",
            "title": "Vertical"
        },
        "fixUpper": {
            "datatype": "string"
        },
        "fixLower": {
            "datatype": "string"
        },
        "natural": {
            "datatype": "boolean"
        },
        "leftBottom": {
            "datatype": "boolean"
        },
        "includeZero": {
            "datatype": "boolean"
        },
        "fixed": {
            "datatype": "boolean"
        },
        "majorLabels": {
            "datatype": "boolean"
        },
        "minorTicks": {
            "datatype": "boolean"
        },
        "minorLabels": {
            "datatype": "boolean"
        },
        "microTicks": {
            "datatype": "boolean"
        },
        "htmlLabels": {
            "datatype": "boolean"
        },
        "min": {
            "datatype": "number",
            "defaultValue": 0
        },
        "max": {
            "datatype": "number",
            "defaultValue": 1
        },
        "from": {
            "datatype": "number",
            "defaultValue": 0
        },
        "to": {
            "datatype": "number",
            "defaultValue": 1
        },
        "majorTickStep": {
            "datatype": "number",
            "defaultValue": 4
        },
        "minorTickStep": {
            "datatype": "number",
            "defaultValue": 2
        },
        "microTickStep": {
            "datatype": "number",
            "defaultValue": 1
        },
        "stroke": {
            "datatype": "string",
            "title": "Stroke"
        },
        "font": {
            "datatype": "string",
            "title": "Font"
        },
        "fontColor": {
            "datatype": "string",
            "title": "Font Color"
        }
    },
    "content": "<span></span>"
}