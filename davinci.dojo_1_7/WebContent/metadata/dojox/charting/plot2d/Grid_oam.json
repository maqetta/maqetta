        "hMinorLines": {
            "datatype": "boolean",
            "defaultValue": "false"
        },
        "vMajorLines": {
            "datatype": "boolean",
            "defaultValue": "true"
        },
        "vMinorLines": {
            "datatype": "boolean",
            "defaultValue": "false"
        }
    },
