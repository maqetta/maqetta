{
    "id": "http://dojotoolkit.org/dojox/charting/plot2d/Grid",
    "name": "dojox.charting.plot2d/Grid",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "css",
