{
    "id": "http://dojotoolkit.org/dojox/charting/plot2d/Lines",
    "name": "dojox.charting.plot2d/Lines",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.charting.plot2d.Lines');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../../"
        }
    },
    "property": {
        "hAxis": {
            "datatype": "string"
        },
        "vAxis": {
            "datatype": "string"
        },
        "markers": {
            "datatype": "boolean",
            "title": "Markers"
        },
        "areas": {
            "datatype": "boolean",
            "title": "Areas"
        },
        "tension": {
            "datatype": "number",
            "title": "Tension"
        },
        "shadows": {
            "datatype": "array",
            "title": "Shadow",
            "description": "Draws shadow. Provide dx, dy, dw separated by commas."
        }
    },
    "content": "<span></span>"
}