{
    "id": "http://dojotoolkit.org/dojox/form/FileInputAuto",
    "name": "dojox.form.FileInputAuto",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojox/form/resources/FileInput.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.form.FileInputAuto');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../"
        }
    },
    "property": {
        "type": {
            "datatype": "string",
            "defaultValue": "text",
            "hidden": true
        },
        "name": {
            "datatype": "string",
            "defaultValue": "uploadFile",
            "title": "Name"
        },
        "alt": {
            "datatype": "string",
            "hidden": true
        },
        "value": {
            "datatype": "string",
            "title": "Value"
        },
        "tabIndex": {
            "datatype": "string",
            "defaultValue": "0",
            "title": "Tab Index"
        },
        "disabled": {
            "datatype": "boolean",
            "title": "Disabled"
        },
        "readOnly": {
            "datatype": "boolean",
            "hidden": true
        },
        "intermediateChanges": {
            "datatype": "boolean",
            "hidden": true
        },
        "label": {
            "datatype": "string",
            "defaultValue": "Browse ...",
            "title": "Label"
        },
        "cancelText": {
            "datatype": "string",
            "defaultValue": "Cancel",
            "title": "Cancel Text"
        },
        "url": {
            "datatype": "string",
            "format": "url",
            "title": "URL"
        },
        "blurDelay": {
            "datatype": "number",
            "defaultValue": 2000,
            "title": "Blur Delay"
        },
        "duration": {
            "datatype": "number",
            "defaultValue": 500,
            "title": "Duration"
        },
        "uploadMessage": {
            "datatype": "string",
            "defaultValue": "Uploading ...",
            "title": "Upload Message"
        },
        "triggerEvent": {
            "datatype": "string",
            "description": "Event which triggers the upload. Defaults to onblur, sending the file selected\n'blurDelay' milliseconds after losing focus. Set to \"onchange\" with a low blurDelay\nto send files immediately after uploading.",
            "hidden": false,
            "defaultValue": "onblur"
        },
        "scrollOnFocus": {
            "datatype": "boolean",
            "description": "On focus, should this widget scroll into view?",
            "hidden": false
        }
    },
    "content": "<input></input>"
}