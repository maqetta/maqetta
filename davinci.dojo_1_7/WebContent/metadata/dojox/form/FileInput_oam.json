{
    "id": "http://dojotoolkit.org/dojox/form/FileInput",
    "name": "dojox.form.FileInput",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojox/form/resources/FileInput.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.form.FileInput');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../"
        }
    },
    "property": {
        "type": {
            "datatype": "string",
            "defaultValue": "text",
            "hidden": true
        },
        "name": {
            "datatype": "string",
            "defaultValue": "uploadFile",
            "title": "Name"
        },
        "alt": {
            "datatype": "string",
            "hidden": true
        },
        "value": {
            "datatype": "string",
            "title": "Value"
        },
        "tabIndex": {
            "datatype": "string",
            "defaultValue": "0",
            "title": "Tab Index"
        },
        "disabled": {
            "datatype": "boolean",
            "title": "Disabled"
        },
        "readOnly": {
            "datatype": "boolean",
            "hidden": true
        },
        "intermediateChanges": {
            "datatype": "boolean",
            "hidden": true
        },
        "label": {
            "datatype": "string",
            "defaultValue": "Browse ...",
            "title": "Label"
        },
        "cancelText": {
            "datatype": "string",
            "defaultValue": "Cancel",
            "title": "Cancel Text"
        },
        "scrollOnFocus": {
            "datatype": "boolean",
            "description": "On focus, should this widget scroll into view?",
            "hidden": false
        }
    },
    "content": "<input></input>"
}