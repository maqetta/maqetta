{
    "id": "http://dojotoolkit.org/dojox/grid/data/DojoData",
    "name": "dojox.grid.data/DojoData",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.grid._data.model');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../../"
        }
    },
    "property": {
        "jsId": {
            "datatype": "string",
            "title": "Model ID"
        },
        "rowsPerPage": {
            "datatype": "number",
            "defaultValue": 100,
            "title": "Rows Per Page"
        },
        "store": {
            "datatype": "object",
            "title": "Data Store"
        },
        "query": {
            "datatype": "json",
            "defaultValue": "\"{\\\"name\\\":\\\"*\\\"}\"",
            "title": "Query"
        },
        "queryOptions": {
            "datatype": "json",
            "title": "Query Options"
        }
    },
    "content": "<span></span>"
}