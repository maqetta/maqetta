{
    "id": "http://dojotoolkit.org/dojox/layout/ContentPane",
    "name": "dojox.layout.ContentPane",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.layout.ContentPane');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../"
        }
    },
    "property": {
        "href": {
            "datatype": "string",
            "format": "url",
            "title": "URL"
        },
        "extractContent": {
            "datatype": "boolean",
            "title": "Extract Content"
        },
        "parseOnLoad": {
            "datatype": "boolean",
            "defaultValue": true,
            "title": "Parse On Load"
        },
        "preventCache": {
            "datatype": "boolean",
            "title": "Prevent Cache"
        },
        "preload": {
            "datatype": "boolean",
            "title": "Preload"
        },
        "refreshOnShow": {
            "datatype": "boolean",
            "title": "Refresh On Show"
        },
        "loadingMessage": {
            "datatype": "string",
            "defaultValue": "<span class='dijitContentPaneLoading'>Loading...</span>",
            "title": "Loading Message"
        },
        "errorMessage": {
            "datatype": "string",
            "defaultValue": "<span class='dijitContentPaneError'>Sorry, an error occurred</span>",
            "title": "Error Message"
        },
        "doLayout": {
            "datatype": "string",
            "defaultValue": "auto",
            "hidden": true
        },
        "adjustPaths": {
            "datatype": "boolean",
            "title": "Adjust Paths"
        },
        "cleanContent": {
            "datatype": "boolean",
            "title": "Clean Content"
        },
        "renderStyles": {
            "datatype": "boolean",
            "title": "Render Styles"
        },
        "executeScripts": {
            "datatype": "boolean",
            "defaultValue": true,
            "title": "Execute Scripts"
        },
        "scriptHasHooks": {
            "datatype": "boolean",
            "title": "Script Has Hooks"
        },
        "content": {
            "datatype": "string",
            "description": "The innerHTML of the ContentPane.\nNote that the initialization parameter / argument to attr(\"content\", ...)\ncan be a String, DomNode, Nodelist, or _Widget.",
            "hidden": false
        },
        "ioArgs": {
            "datatype": "json",
            "description": "makes it possible to add custom args to xhrGet, like ioArgs.headers['X-myHeader'] = 'true'",
            "hidden": false
        },
        "isContainer": {
            "datatype": "boolean",
            "description": "Just a flag indicating that this widget will call resize() on\nits children.   _LayoutWidget based widgets check for\n\n\t\tif(!this.getParent || !this.getParent()){\n\nand if getParent() returns false because !parent.isContainer,\nthen they resize themselves on initialization.",
            "defaultValue": true,
            "hidden": true
        }
    },
    "content": "<span></span>"
}