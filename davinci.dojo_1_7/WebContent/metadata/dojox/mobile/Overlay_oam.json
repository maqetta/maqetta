{
    "id": "http://dojotoolkit.org/dojox/mobile/Overlay",
    "name": "dojox.mobile.Overlay",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.parser');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.deviceTheme');"
        },
        {
            "type": "javascript",
            "$text": "dojo.requireIf(!dojo.isWebKit, 'dojox.mobile.compat');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.Overlay');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../"
        }
    },
    "property": {
        "baseClass": {
            "datatype": "string",
            "defaultValue": "mblOverlay mblOverlayHidden"
        }
    },
    "content": "<div></div>"
}