{
    "id": "http://dojotoolkit.org/dojox/mobile/ScrollableView",
    "name": "dojox.mobile.ScrollableView",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.parser');"
        },
        {
            "type": "javascript",
            "$text": "dojo.requireIf(!dojo.isWebKit, 'dojox.mobile.compat');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.ScrollableView');"
        },
        {
            "type": "css",
            "src": "dojox/mobile/themes/iphone/iphone.css",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../"
        }
    },
    "property": {
        "selected": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "keepScrollPos": {
            "datatype": "boolean",
            "defaultValue": true
        },
        "scrollBar": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "scrollDir": {
            "datatype": "string",
            "defaultValue": "v"
        },
        "fadeScrollBar": {
            "datatype": "boolean",
            "defaultValue": true
        },
        "disableFlashScrollBar": {
            "datatype": "boolean",
            "defaultValue": false
        }
    },
    "content": "<div></div>"
}