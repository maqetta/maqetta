{
    "id": "http://dojotoolkit.org/dojox/mobile/Switch",
    "name": "dojox.mobile.Switch",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.parser');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.deviceTheme');"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.mobile.compat');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../"
        }
    },
    "property": {
        "value": {
            "datatype": "string",
            "defaultValue": "on"
        },
        "leftLabel": {
            "datatype": "string",
            "defaultValue": "ON"
        },
        "rightLabel": {
            "datatype": "string",
            "defaultValue": "OFF"
        }
    },
    "content": "<div></div>"
}