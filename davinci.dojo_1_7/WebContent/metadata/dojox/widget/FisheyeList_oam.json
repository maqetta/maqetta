{
    "id": "http://dojotoolkit.org/dojox/widget/FisheyeList",
    "name": "dojox.widget.FisheyeList",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojox/widget/FisheyeList/FisheyeList.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.widget.FisheyeList');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../"
        }
    },
    "property": {
        "itemWidth": {
            "datatype": "number",
            "defaultValue": 40,
            "title": "Item Width"
        },
        "itemHeight": {
            "datatype": "number",
            "defaultValue": 40,
            "title": "Item Height"
        },
        "itemMaxWidth": {
            "datatype": "number",
            "defaultValue": 150,
            "title": "Item Maximum Width"
        },
        "itemMaxHeight": {
            "datatype": "number",
            "defaultValue": 150,
            "title": "Item Maximum Height"
        },
        "orientation": {
            "datatype": "string",
            "option": [
                {
                    "value": "horizontal"
                },
                {
                    "value": "vertical"
                }
            ],
            "defaultValue": "horizontal",
            "title": "Show RBG"
        },
        "isFixed": {
            "datatype": "boolean",
            "title": "Fixed"
        },
        "conservativeTrigger": {
            "datatype": "boolean",
            "title": "Conservative Trigger"
        },
        "effectUnits": {
            "datatype": "number",
            "defaultValue": 2,
            "title": "Effect Units"
        },
        "itemPadding": {
            "datatype": "number",
            "defaultValue": 10,
            "title": "Item Padding"
        },
        "attachEdge": {
            "datatype": "string",
            "option": [
                {
                    "value": "center"
                },
                {
                    "value": "left"
                },
                {
                    "value": "right"
                },
                {
                    "value": "top"
                },
                {
                    "value": "bottom"
                }
            ],
            "defaultValue": "center",
            "title": "Attach Edge"
        },
        "labelEdge": {
            "datatype": "string",
            "option": [
                {
                    "value": "center"
                },
                {
                    "value": "left"
                },
                {
                    "value": "right"
                },
                {
                    "value": "top"
                },
                {
                    "value": "bottom"
                }
            ],
            "defaultValue": "bottom",
            "title": "Label Edge"
        },
        "isContainer": {
            "datatype": "boolean",
            "description": "Just a flag indicating that this widget descends from dijit._Container",
            "defaultValue": "true",
            "hidden": true
        }
    },
    "content": "<span></span>"
}