{
    "id": "http://dojotoolkit.org/dojox/widget/Loader",
    "name": "dojox.widget.Loader",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript",
            "src": "dojo/dojo.js",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojo/resources/dojo.css",
            "$library": "dojo"
        },
        {
            "type": "css",
            "src": "dojox/widget/Loader/Loader.css",
            "$library": "dojo"
        },
        {
            "type": "javascript",
            "$text": "dojo.require('dojox.widget.Loader');"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../"
        }
    },
    "property": {
        "loadMessage": {
            "datatype": "string",
            "defaultValue": "Loading ...",
            "title": "Load Message"
        },
        "hasVisuals": {
            "datatype": "boolean",
            "defaultValue": true,
            "title": "Has Visuals"
        },
        "attachToPointer": {
            "datatype": "boolean",
            "defaultValue": true,
            "title": "Attach To Pointer"
        },
        "duration": {
            "datatype": "number",
            "defaultValue": 125,
            "title": "Duration"
        }
    },
    "content": "<span></span>"
}