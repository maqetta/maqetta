{
    "id": "http://dojotoolkit.org/dijit/tree/Calendar",
    "name": "dijit.tree.Calendar",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dijit/Calendar",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "name": {
            "datatype": "string",
            "title": "Name"
        },
        "value": {
            "datatype": "string",
            "format": "date",
            "title": "Value"
        },
        "dayWidth": {
            "datatype": "string",
            "option": [
                {
                    "value": "narrow"
                },
                {
                    "value": "wide"
                },
                {
                    "value": "abbr"
                }
            ],
            "defaultValue": "narrow",
            "title": "Day Width"
        },
        "datePackage": {
            "datatype": "string",
            "description": "JavaScript namespace to find Calendar routines.  Uses Gregorian Calendar routines\nat dojo.date by default.",
            "hidden": false
        }
    },
    "content": "<input></input>"
}