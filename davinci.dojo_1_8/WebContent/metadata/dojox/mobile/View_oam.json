{
    "id": "http://dojotoolkit.org/dojox/mobile/View",
    "name": "dojox.mobile.View",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/compat",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojo/hash",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "selected": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "keepScrollPos": {
            "datatype": "boolean",
            "defaultValue": true
        }
    },
    "content": "<div></div>"
}